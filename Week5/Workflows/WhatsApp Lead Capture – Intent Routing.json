    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const outerBody = item.json.body || {};        // the top-level \"body\" from webhook\n  const innerBody = outerBody.body || outerBody; // if message is nested in body.body, use it\n\n  const parsed = innerBody.parsed || {};\n  const name = parsed.name || null;\n  const phone = parsed.phone || outerBody.from || null; // phone may be in parsed or in outer 'from'\n  const service_interest = parsed.service_interest || null;\n  const raw_message = innerBody.message || outerBody.message || null;\n\n  return {\n    json: {\n      name,\n      phone,\n      service_interest,\n      raw_message\n    }\n  };\n});\n"
      },
      "type": "n8n-nodes-base.code",
