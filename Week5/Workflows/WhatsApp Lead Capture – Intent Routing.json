      "id": "988f3836-9ce5-4342-b261-612c6c8fecaa",
      "name": "Map Lead Data"
    },
    {
      "parameters": {
        "jsCode": "// Classify Intent code for n8n Code node\nreturn items.map(item => {\n  const raw = (item.json.raw_message || \"\").toString().toLowerCase();\n  const interest = (item.json.service_interest || \"\").toString().toLowerCase();\n\n  // keyword sets (expand later if you want)\n  const salesKeywords = [\"buy\",\"price\",\"purchase\",\"cost\",\"order\",\"demo\",\"quote\",\"pricing\",\"subscribe\",\"price list\"];\n  const supportKeywords = [\"help\",\"issue\",\"problem\",\"error\",\"not working\",\"support\",\"refund\",\"cancel\",\"return\",\"bug\",\"complaint\"];\n\n  // simple match\n  let intent = \"General Inquiry\";\n  for (const k of salesKeywords) if (raw.includes(k) || interest.includes(k)) { intent = \"Sales\"; break; }\n  if (intent === \"General Inquiry\") {\n    for (const k of supportKeywords) if (raw.includes(k)) { intent = \"Support\"; break; }\n  }\n\n  return {\n    json: {\n      ...item.json,\n      intent\n    }\n  };\n});\n"
      },
